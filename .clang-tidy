Checks: >
  *,
  -*-magic-numbers,
  -*-narrowing-conversions,
  -*-uppercase-literal-suffix,
  -bugprone-branch-clone,
  -bugprone-suspicious-string-compare,
  -cert-err34-c,
  -clang-analyzer-unix.Malloc,
  -hicpp-signed-bitwise,
  -llvm-header-guard,
  -readability-else-after-return,
WarningsAsErrors: '*'
